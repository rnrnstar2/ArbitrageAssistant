{
  "name": "arbitrage-assistant",
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "start": "./scripts/start-all.sh",
    "president": "./scripts/president/start.sh",
    "team": "./scripts/team/start.sh",
    "president:clean": "./scripts/president/clean-start.sh",
    "team:clean": "./scripts/team/clean-start.sh",
    "clean:all": "./scripts/clean-all.sh",
    "president:connect": "tmux attach -t president",
    "team:connect": "tmux attach -t team",
    "dev": "turbo dev --parallel",
    "dev:admin": "turbo dev --filter=admin",
    "build": "turbo build --filter=!@repo/shared-backend",
    "lint": "turbo lint --continue",
    "test": "turbo test --continue",
    "predev": "npm run sync:amplify",
    "postinstall": "npm run sync:amplify",
    "sync:amplify": "./scripts/backend/sync-amplify-outputs.sh"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.5.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=20.15.0",
    "npm": ">=9.8.0"
  },
  "packageManager": "npm@9.8.0"
}